<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pm.pmapi.dao.TagDao">
    <resultMap id="BaseResultMap" type="com.pm.pmapi.dto.TagInfo">
        <id column="tag_id" jdbcType="BIGINT" property="tagId"/>
        <result column="tag" jdbcType="VARCHAR" property="tagName"/>
    </resultMap>
    <select id="geTagByTagId" resultMap="BaseResultMap">
        select tag_id, tag
        from tab_tag where tag_id=#{tagId}
    </select>
</mapper>